{
    "name": "Each Second matters",
    "description": "Spend time wisely!",
    "version": "1.0",
    "manifest_version": 2,
    "icons": { 
     "128": "icon.png" },
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["alert.js","content.js"]
        }
      ],
      "background": {
        "scripts": ["background.js"]
      },
      "permissions": [
        "tabs"
      ]
  }